
#es-client
za.es.marvel.server=localhost
za.es.marvel.cluster.name=elasticsearch-dev
za.es.marvel.cluster.node.name=marvel-node-1
#es bulk
za.es.marvel.bulk.actions=1000
za.es.marvel.bulk.request=2
#flush size,MB
za.es.marvel.bulk.size=5

#hdfs
za.es.marvel.hdfs.uri=hdfs://localhost:9000/
za.es.marvel.hdfs.fetch.size=1000

#claim
za.es.marvel.claim.hdfs.path=user/admin/search4only_es
#0,1,2
za.es.marvel.claim.hdfs.shard=0
za.es.marvel.claim.hdfs.file.name=search4only
za.es.marvel.claim.index=only-test
za.es.marvel.claim.type=only
za.es.marvel.claim.id=id




